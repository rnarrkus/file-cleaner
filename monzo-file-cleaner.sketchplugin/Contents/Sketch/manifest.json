{
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icon.png",
  "commands": [
    {
      "name": "👮‍♀️ Validate",
      "description": "Check that Monzo conventions are being followed 🤓",
      "identifier": "monzo.validate",
      "script": "validate.js",
      "shortcut": "ctrl shift m"
    },
    {
      "name": "🧙‍♀️ Validate and fix",
      "description": "Apply Monzo conventions automatically 🧙‍♀️",
      "identifier": "monzo.validate-and-fix",
      "script": "validate-and-fix.js",
      "shortcut": "ctrl shift l"
    },
    {
      "name": "🧙‍♀️ Validate and fix big",
      "description": "Apply Monzo conventions automatically 🧙‍♀️",
      "identifier": "monzo.validate-and-fix-big",
      "script": "validate-and-fix-big.js",
      "shortcut": "ctrl shift k"
    }
  ],
  "menu": {
    "title": "🏦 Monzo",
    "items": [
      "monzo.validate",
      "monzo.validate-and-fix",
      "monzo.validate-and-fix-big"
    ]
  },
  "version": "1.0.0",
  "description": "A plugin that keeps your Sketch files in perfect shape.",
  "name": "Monzo File Cleaner",
  "identifier": "Monzo File Cleaner",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com//master/.appcast.xml",
  "author": "Monzo",
  "authorEmail": "harry@monzo.com"
}